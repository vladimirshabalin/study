                                        input(type="checkbox" id="catalog-filter-size-s")
                                        label(for="catalog-filter-size-s") s
                                    .catalog-filter-col
                                        input(type="checkbox" id="catalog-filter-size-m")
                                        label(for="catalog-filter-size-m") m
                                .catalog-filter-row
                                    .catalog-filter-col
                                        input(type="checkbox" id="catalog-filter-size-l")
                                        label(for="catalog-filter-size-l") l
                                    .catalog-filter-col
                                        input(type="checkbox" id="catalog-filter-size-xl")
                                        label(for="catalog-filter-size-xl") xl
                                    .catalog-filter-col
                                        input(type="checkbox" id="catalog-filter-size-xxl")
                                        label(for="catalog-filter-size-xxl") xxl
                            .catalog-filter-col
                                p.catalog-filter-title price
                                .catalog-filter-row
                                    input(type="range" min="0" max="100" value="30")
                                .catalog-filter-row.catalog-filter-range-price
                                    span.catalog-filter-range-text $52
                                    span.catalog-filter-range-text $400
                        .catalog-filter-sort
                            span sort by
                            input(list="catalog-sort-by" placeholder="name")
                            datalist(id="catalog-sort-by")
                                option(value="name")
                                option(value="price")
                                option(value="size")
                            span show items
                            input.sort-show-items(list="catalog-sort-quantity" placeholder="09")
                            datalist(id="catalog-sort-quantity")
                                option(value="9")
                                option(value="18")
                                option(value="27")
