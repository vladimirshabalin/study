                                    a.catalog-filter-text lace
                                    span |
                                    a.catalog-filter-text boherman
                            .catalog-filter-col
                                p.catalog-filter-title size
                                .catalog-filter-row
                                    .catalog-filter-col
                                        input(type="checkbox" id="catalog-filter-size-xxs")
                                        label(for="catalog-filter-size-xxs") xxs
                                    .catalog-filter-col
                                        input(type="checkbox" id="catalog-filter-size-xs")
                                        label(for="catalog-filter-size-xs") xs
